<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE icons [
        <!ELEMENT icons (file)*>
        <!ELEMENT file (row)*>
        <!ATTLIST file
                name CDATA #REQUIRED
                prefix CDATA #IMPLIED
                cellwidth CDATA #REQUIRED
                cellheight CDATA #REQUIRED>
        <!ELEMENT row (icon)*>
        <!ELEMENT icon (#PCDATA)>
        ]>
<icons>
    <!--
    <file
         name= file name (relative to res/)
         (optional) prefix= prepended to icon ids>
     -->

    <!-- Item icons, iconId = "I_" + itemId -->
    <file name="icons/items.png" prefix="I_" cellwidth="36" cellheight="36">
        <row>
            <icon>IncubiD</icon>
        </row>
    </file>

    <!-- Ability icons, iconId = "A_" + abilityId -->
    <!--
     <file name="icons/abilities.png" prefix="A_" cellwidth="36" cellheight="36">
     </file>
     -->
</icons>
